#**************************************************************
# Time Information
#**************************************************************
set_time_format -unit ns -decimal_places 3  		
#**************************************************************
# Create Clock
#************************************************************** 		
create_clock -name {clk} -period 20.000 -waveform { 0.000 10.000 } [get_ports {clk}]
#**************************************************************
# False Paths
#**************************************************************
set_false_path -from [get_ports {x[*] x_dv rst_b}]
set_false_path -to [get_ports {y_dv y[*]}]